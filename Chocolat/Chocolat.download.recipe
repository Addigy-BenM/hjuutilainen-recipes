<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the current release version of Chocolat.</string>
	<key>Identifier</key>
	<string>io.github.hjuutilainen.download.Chocolat</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>Chocolat</string>
		<key>SPARKLE_FEED_URL</key>
		<string>https://chocolatapp.com/userspace/appcast/appcast_alpha.php</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.3.1</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Processor</key>
			<string>DeprecationWarning</string>
			<key>Arguments</key>
			<dict>
				 <key>warning_message</key>
				 <string>This recipe has been deprecated and will be removed soon</string>
			</dict>
		</dict>
	</array>
</dict>
</plist>
