<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the latest version of GIMP from gimp.org.</string>
	<key>Identifier</key>
	<string>io.github.hjuutilainen.download.GIMP</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>GIMP</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.5.2</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Processor</key>
			<string>CURLTextSearcher</string>
			<key>Arguments</key>
			<dict>
				<key>re_pattern</key>
				<string>href="//download.gimp.org/mirror/pub/gimp/v([\d\.]+)/osx/gimp-[\d\.]+-x86_64.dmg"</string>
				<key>result_output_var_name</key>
				<string>major_version</string>
				<key>url</key>
				<string>https://www.gimp.org/downloads/</string>
			</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>CURLTextSearcher</string>
			<key>Arguments</key>
			<dict>
				<key>re_pattern</key>
				<string>href="//download.gimp.org/mirror/pub/gimp/v[\d\.]+/osx/gimp-([\d\.]+)-x86_64.dmg"</string>
				<key>result_output_var_name</key>
				<string>full_version</string>
				<key>url</key>
				<string>https://www.gimp.org/downloads/</string>
			</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>CURLDownloader</string>
			<key>Arguments</key>
			<dict>
				<key>filename</key>
				<string>%NAME%-%full_version%.dmg</string>
				<key>url</key>
				<string>https://download.gimp.org/mirror/pub/gimp/v%major_version%/osx/gimp-%full_version%-x86_64.dmg</string>
			</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>EndOfCheckPhase</string>
		</dict>
	</array>
</dict>
</plist>
